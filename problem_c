#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;cin>>t;
    int cse = 1;
    while(t--){
        long long n;
        cin >> n;
        int one = 0;
        while(n){
            if(n%2==1){
                one++;
            }
            n/=2;
        }
        if(one%2==1){
            cout << "Case " << cse++ << ": odd" << endl;
        }else{
            cout << "Case " << cse++ << ": even" << endl;
        }
    }
    return 0;
}
