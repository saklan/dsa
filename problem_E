#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    int ch = 1;
    while(t--){
        int n;
        cin >> n;
        int sum = 0;
        cout << "Case " << ch++ << ":" << endl;
        for(int i = 1; i <= n; i++){
            string s;
            cin >> s;
            if(s=="donate"){
                int val;
                cin >> val;
                sum+=val;
            }
            if(s=="report") cout << sum << endl;
        }

    }

    return 0;
}
