#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;cin>>t;
    int cas = 1;
    while(t--){
        string s;cin>>s;
        string rev = s;
        reverse(s.begin(),s.end());
        if(s==rev){
            cout << "Case " << cas++ << ": Yes" << endl;
        }else{
            cout << "Case " << cas++ << ": No" << endl;
        }
    }
    return 0;
}
